model LibraryYear_ListAV {
    libraryyear_id Int
    listav_id      Int
    LibraryYearRef Library_Year? @relation(fields: [libraryyear_id], references: [id])

    @@unique([libraryyear_id, listav_id])
}

model LibraryYear_ListEBook {
    libraryyear_id Int
    listebook_id   Int
    LibraryYearRef Library_Year? @relation(fields: [libraryyear_id], references: [id])

    @@unique([libraryyear_id, listebook_id])
}

model LibraryYear_ListEJournal {
    libraryyear_id  Int
    listejournal_id Int
    LibraryYearRef  Library_Year? @relation(fields: [libraryyear_id], references: [id])

    @@unique([libraryyear_id, listejournal_id])
}
