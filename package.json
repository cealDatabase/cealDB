    "postinstall": "prisma generate --schema=./prisma/schema",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "schema": "./prisma/schema",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
